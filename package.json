    "bugs": {
        "url": "https://github.com/minhkashyu/nightlife-coordination-backend/issues"
    },
    "homepage": "https://github.com/minhkashyu/nightlife-coordination-backend#readme",
    "dependencies": {
        "babel-cli": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "body-parser": "^1.17.1",
        "cross-env": "^5.0.5",
        "dotenv": "^4.0.0",
        "express": "^4.15.2",
        "jsonwebtoken": "^7.3.0",
        "moment": "^2.18.1",
        "mongoose": "^4.8.6",
        "morgan": "^1.8.1",
        "nodemon": "^1.11.0",
        "passport": "^0.3.2",
