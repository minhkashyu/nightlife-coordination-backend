{
    "name": "nightlife-coordination-backend",
    "version": "1.0.0",
    "description": "Back End of Nightlife Coordination App - A Free Code Camp Dymamic Web Application Project.",
    "engines": {
        "node": "6.11.x"
    },
    "main": "server.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production babel-node server.js",
        "dev": "cross-env NODE_ENV=development babel-node server.js",
